{
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "^3.1",
        "symfony/flex": "^1.21"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
